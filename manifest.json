{
  "name": "Life Gamification",
  "version": "0.1",
  "description": "Measure your progress in your real life skills!",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Life Gamification",
      "default_icon": "assets/icon.png",
      "default_popup": "src/popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
